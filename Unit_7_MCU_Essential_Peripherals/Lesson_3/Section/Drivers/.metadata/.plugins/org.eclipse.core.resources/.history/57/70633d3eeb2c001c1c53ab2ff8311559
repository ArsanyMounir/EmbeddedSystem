#ifndef INC__STM32F103C6_GPIO_DRIVER_H_
#define INC__STM32F103C6_GPIO_DRIVER_H_

//-----------------------------
//Includes
//-----------------------------
#include "STM32F103C6.h"


//==============================================================


//-----------------------------
//Config Structure
//-----------------------------
typedef struct
{
	uint16_t GPIO_PinNumber ;	//Specifies the GPIO Pins to be Configured
								//This Parameter must be set at Macro Configuration Manual GPIO_Pins_Define

	uint8_t GPIO_Mode ;			//Specifies the Operation mode of the selected pins
								//This Parameter must be set at Macro Configuration Manual GPIO_MODE_Define



}GPIO_PinConfig_t;




//==============================================================


//-----------------------------
//Macro Configuration Manual
//-----------------------------




#endif /* INC__STM32F103C6_GPIO_DRIVER_H_ */
