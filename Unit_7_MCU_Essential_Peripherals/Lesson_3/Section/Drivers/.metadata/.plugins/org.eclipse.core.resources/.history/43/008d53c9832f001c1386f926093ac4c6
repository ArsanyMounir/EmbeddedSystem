#ifndef INC_KEYPAD_H_
#define INC_KEYPAD_H_

#include "STM32F103C6_GPIO_Driver.h"
#include "STM32F103C6.h"



#define KEYPAD_PORT PORTD
#define DDIR_KEYPAD_PORT DDRD
#define KEYPAD_PIN PIND

#define R0 0
#define R1 1
#define R2 2
#define R3 3
#define C0 4
#define C1 5
#define C2 6
#define C3 7


void KEYPAD_init();
char KEYPAD_CHAR();


#endif /* INC_KEYPAD_H_ */
